@using Catalogo.MeuNamespace
@model IEnumerable<Filme>

<style>
    td {
        border:1px solid black;
        padding: 5px;
    }
</style>

<table>
    <tr>
        <th>Código</th>
        <th>Titulo</th>
        <th>Sinopse</th>
        <th>Data de Lançamento</th>
        <th>Categoria</th>
        <th>Em Cartaz</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Codigo</td>
            <td>@item.Titulo</td>
            <td>@item.Sinopse</td>
            <td>@item.Lancamento.ToShortDateString()</td>
            <td>@item.Categoria</td>
            <td>@item.EmCartaz</td>
        </tr>
    }
</table>